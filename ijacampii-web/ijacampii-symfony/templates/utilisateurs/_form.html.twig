
{{ form_start(form) }}
<div class="row">
    <div class="col-md-4">{{ form_row(form.nom)}}</div>
    <div class="col-md-4">{{ form_row(form.prenom)}}</div>
    <div class="col-md-4">{{ form_row(form.dateNaissance)}}</div>
</div>
<div class="row">
    <div class="col-md-4">{{ form_row(form.adresse)}}</div>
    <div class="col-md-4">{{ form_row(form.numTel)}}</div>
    <div class="col-md-4">{{ form_row(form.login)}}</div>

</div>
{{form_widget(form)}}
<button class="btn btn-primary">{{button | default('Ajouter')}}</button>
{{ form_end(form) }}
</div>

